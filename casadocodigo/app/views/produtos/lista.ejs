<html>

<body>
    <table border="1px">
        <tr>
            <th>ID</th>
            <th>Título</th>
            <th>Descricao</th>
            <th>Preço</th>
        </tr>

        <%for(var i=0;i<lista.length; i++){%>
            <tr>
                <td>
                    <%=lista[i].id;%>
                </td>
                <td>
                    <%=lista[i].titulo;%>
                </td>
                <td>
                    <%=lista[i].descricao;%>
                </td>
                <td>
                    <%=lista[i].preco;%>
                </td>
            </tr>
            <%}%>

    </table>
</body>

</html>